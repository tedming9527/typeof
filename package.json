{
  "name": "typeof",
  "version": "1.1.4",
  "repository": "https://github.com/tedming9527/typeof.git",
  "author": "tedming9527 <tedming9527@gmail.com>",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "commit": "cz",
    "build": "rollup -c",
    "test": "jest",
    "release": "HUSKY=0 standard-version",
    "prepare": "husky",
    "add-husky-pre-commit": "echo npx lint-staged > .husky/pre-commit",
    "add-husky-commit-msg": "echo npx --no -- commitlint --edit \"$1\" > .husky/commit-msg",
    "add-husky-prepare-commit-msg": "echo \"exec < /dev/tty && npx cz --hook || true\" > .husky/prepare-commit-msg"
  },
  "lint-staged": {
    "*.(js|jsx|ts|tsx)": "eslint --cache --fix"
  },
  "files": [
    "src",
    "dist"
  ],
  "main": "./dist/bundle.js",
  "module": "./dist/bundle.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    "*": {
      "import": "./dist/bundle.mjs",
      "require": "./dist/bundle.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/plugin-transform-runtime": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "commitizen": "^4.3.0",
    "commitlint": "^19.3.0",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^9.5.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.7",
    "prettier": "^3.3.2",
    "rollup": "^4.18.0",
    "standard-version": "^9.5.0",
    "tslib": "^2.6.3",
    "typescript": "^5.4.5"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {}
}
